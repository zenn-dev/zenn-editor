    "history": "^5.0.0",
    "image-size": "^1.0.0",
    "jest": "^27.2.1",
    "js-yaml": "^4.1.0",
    "node-fetch": "^2.6.7",
    "node-loader": "^2.0.0",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5",
