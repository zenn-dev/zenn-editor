{
  "name": "zenn-model",
  "version": "0.3.2-alpha.0",
  "license": "MIT",
  "description": "Model utils for Zenn contents",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zenn-dev/zenn-editor.git",
    "directory": "packages/zenn-model"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "build": "pnpm clean && run-p build:*",
    "build:src": "esbuild ./src/*.ts --format=cjs --outdir=lib",
    "build:types": "tsc --project ./tsconfig.build.json",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier .",
    "lint-strict": "run-s lint-strict:*",
    "lint-strict:eslint": "eslint . --max-warnings 0",
    "lint-strict:prettier": "prettier --check .",
    "fix": "run-s fix:*",
    "fix:eslint": "eslint . --fix",
    "fix:prettier": "prettier -w .",
    "test": "vitest run"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "esbuild": "^0.27.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vitest": "^4.0.6"
  },
  "dependencies": {
    "cheerio": "^1.1.2",
    "emoji-regex": "^10.6.0"
  }
}
