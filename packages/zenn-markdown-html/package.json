{
  "name": "zenn-markdown-html",
  "version": "0.3.2-alpha.0",
  "license": "MIT",
  "description": "Convert markdown to zenn flavor html.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/zenn-dev/zenn-editor.git",
    "directory": "packages/zenn-markdown-html"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "default": "./dist/utils/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "utils": [
        "./dist/utils/index.d.ts"
      ]
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup && tsc",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier .",
    "lint-strict": "run-s lint-strict:*",
    "lint-strict:eslint": "eslint . --max-warnings 0",
    "lint-strict:prettier": "prettier --check .",
    "fix": "run-s fix:*",
    "fix:eslint": "eslint . --fix",
    "fix:prettier": "prettier -w .",
    "test": "vitest run"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^24.9.1",
    "@types/sanitize-html": "^2.16.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "node-html-parser": "^7.0.1",
    "rimraf": "^6.0.1",
    "tsup": "^8.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vitest": "^4.0.6"
  },
  "dependencies": {
    "@steelydylan/markdown-it-imsize": "^1.0.2",
    "cheerio": "1.1.2",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-container": "^4.0.0",
    "markdown-it-footnote": "^4.0.0",
    "markdown-it-inline-comments": "^1.0.1",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-task-lists": "^2.1.1",
    "sanitize-html": "^2.17.0",
    "shiki": "^3.21.0"
  },
  "gitHead": "7da0b06004cf615e42e475de47011c4670eb7318",
  "publishConfig": {
    "access": "public"
  }
}
