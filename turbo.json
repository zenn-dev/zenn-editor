{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "clean": {
      "cache": false
    },
    "test": {
      "outputs": []
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "lib/**"]
    },
    "lint": {
      "outputs": []
    },
    "lint-strict": {
      "outputs": []
    },
    "fix": {
      "outputs": ["src/**"]
    }
  }
}
